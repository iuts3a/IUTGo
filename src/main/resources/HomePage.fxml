<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="0.0"
            minWidth="0.0"
            prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="IUTGo.Controllers.HomePageController">
    <children>
        <Label layoutX="222.0" layoutY="330.0" text="Let's Travel Together !" textFill="CORNFLOWERBLUE">
            <font>
                <Font name="Cooper Black" size="35.0"/>
            </font>
        </Label>
        <ImageView fitHeight="177.0" fitWidth="300.0" layoutX="250.0" layoutY="98.0">
            <image>
                <Image url="@./img/iutGo.png"/>
            </image>
        </ImageView>
        <Button fx:id="btnConnexion" defaultButton="true" layoutX="467.0" layoutY="442.0" mnemonicParsing="false"
                onAction="#btnConnexion_onAction"
                style="-fx-background-color: None; -fx-border-color: CornFlowerBlue; -fx-border-radius: 8; -fx-border-width: 2;"
                text="Sign in" textFill="CORNFLOWERBLUE">
            <font>
                <Font size="24.0"/>
            </font>
        </Button>
        <Button fx:id="btnInscription" defaultButton="true" layoutX="222.0" layoutY="442.0" mnemonicParsing="false"
                onAction="#btnInscription_onAction"
                style="-fx-background-color: None; -fx-border-color: CornFlowerBlue; -fx-border-radius: 8; -fx-border-width: 2;"
                text="Sign up" textFill="CORNFLOWERBLUE">
            <font>
                <Font size="24.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
