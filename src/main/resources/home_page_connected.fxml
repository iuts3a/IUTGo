<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="IUTGo.Controllers.HomePageConnectedController">
   <children>
      <Label layoutX="439.0" layoutY="89.0" text="IUTGo">
         <font>
            <Font name="System Bold" size="41.0" />
         </font>
      </Label>
      <Label layoutX="414.0" layoutY="158.0" text="Qu'est ce qu'IUT Go ?" textOverrun="CLIP" />
      <Label layoutX="14.0" layoutY="209.0" prefHeight="32.0" prefWidth="621.0" text="IUT GO est une application qui sert à créer le RoadTrip que vous souhaitez !" />
      <Label layoutX="14.0" layoutY="250.0" text="Seul ? En groupe ? Vous pouvez choisir si vous voulez accueillir des personnes que vous ne connaissez pas." wrapText="true" />
      <Label layoutX="14.0" layoutY="289.0" text="Faites des nouvelles rencontre et visitez de nouveau lieu grâce à IUTGo !" />
      <Label layoutX="309.0" layoutY="347.0" text="Let's Travel Together !" textFill="#72a0d7">
         <font>
            <Font name="Cooper Black" size="35.0" />
         </font>
      </Label>
      <HBox layoutX="14.0" layoutY="408.0" prefHeight="237.0" prefWidth="975.0">
         <children>
            <Button fx:id="btn_create_rt" mnemonicParsing="false" onAction="#create_rt" prefHeight="237.0" prefWidth="243.0" text="Créer un RoadTrip">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="btn_see_rt" mnemonicParsing="false" onAction="#see_rt" prefHeight="237.0" prefWidth="244.0" text="Voir les RoadTrips disponibles" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="btn_search_pi" mnemonicParsing="false" onAction="#search_pi" prefHeight="237.0" prefWidth="243.0" text="Chercher un point d'intérêt" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Button fx:id="btn_modify_rt" mnemonicParsing="false" onAction="#modify_rt" prefHeight="238.0" prefWidth="243.0" text="Modifier un de mes RoadTrips" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="399.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="129.0" fitWidth="200.0" layoutX="395.0" layoutY="21.0">
         <image>
            <Image url="@/img/iutGo.png" />
         </image>
      </ImageView>
      <Hyperlink fx:id="hl_deco" layoutX="835.0" layoutY="4.0" onAction="#deco" text="Se déconnecter" />
      <Hyperlink fx:id="hl_info" layoutX="835.0" layoutY="39.0" onAction="#informations" text="Mes informations" />
   </children>
</AnchorPane>
