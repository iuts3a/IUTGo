    <children>
        <TextField fx:id="txtLastName" layoutX="270.0" layoutY="222.0" prefHeight="26.0" prefWidth="250.0"
                   promptText="Only letters allowed"/>
        <TextField fx:id="txtFirstName" layoutX="270.0" layoutY="262.0" prefHeight="26.0" prefWidth="250.0"
                   promptText="Only letters allowed"/>
        <TextField fx:id="txtUserName" layoutX="270.0" layoutY="302.0" prefHeight="26.0" prefWidth="250.0"
                   promptText="Must be minimum 6 characters"/>
        <TextField fx:id="txtEmail" layoutX="270.0" layoutY="342.0" prefHeight="26.0" prefWidth="250.0"
                   promptText="Valid Email"/>
        <PasswordField fx:id="txtPassword1" layoutX="270.0" layoutY="382.0" prefHeight="26.0" prefWidth="250.0"
                       promptText="Strong Password"/>
        <PasswordField fx:id="txtPassword2" layoutX="270.0" layoutY="422.0" prefHeight="26.0" prefWidth="250.0"
                       promptText="Must match previous Password"/>
        <TextField fx:id="txtXpos" layoutX="270.0" layoutY="462.0" prefHeight="26.0" prefWidth="70.0"
                   promptText="Xpos"/>
        <TextField fx:id="txtYpos" layoutX="350.0" layoutY="462.0" prefHeight="26.0" prefWidth="70.0"
                   promptText="Ypos"/>
        <TextField fx:id="txtTown" layoutX="430.0" layoutY="462.0" prefHeight="26.0" prefWidth="90.0"
                   promptText="Town"/>
