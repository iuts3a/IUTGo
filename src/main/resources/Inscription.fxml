            <padding>
                <Insets right="15.0"/>
            </padding>
        </Label>
        <Label layoutX="147.0" layoutY="462.0" prefHeight="26.0" text="Coordinates :" AnchorPane.rightAnchor="530.0">
            <padding>
                <Insets right="15.0"/>
            </padding>
        </Label>
        <Label fx:id="error_user" layoutX="520.0" layoutY="302.0" prefHeight="26.0" textFill="RED"
