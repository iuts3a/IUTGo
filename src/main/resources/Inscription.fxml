            </padding>
        </Label>
        <Label layoutX="183.0" layoutY="262.0" prefHeight="26.0" text="First Name :" AnchorPane.rightAnchor="530.0">
            <padding>
                <Insets right="15.0"/>
            </padding>
        </Label>
        <Label layoutX="177.0" layoutY="302.0" prefHeight="26.0" text="User Name :" AnchorPane.rightAnchor="530.0">
